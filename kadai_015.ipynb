      "source": [
        "#クラスを定義\n",
        "class Human:\n",
        "  #要素を定義\n",
        "  def __init__(self, name, age):\n",
        "     self.name = name\n",
        "     self.age = age\n",
        "  #メソッドを定義\n",
        "  def printinfo(self):\n",
        "    print(f\"名前: {self.name}, 年齢: {self.age}\")\n",
        "\n",
        "#インスタンス化\n",
        "output = Human(\"侍太郎\", 25)\n",
        "output.printinfo()"
      ]
    }
