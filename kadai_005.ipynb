        "# 台形の面積を求める\n",
        "top_side = 10\n",
        "bottom_side = 20\n",
        "height = 5\n",
        "area = ((top_side + bottom_side) * height / 2)\n",
        "\n",
        "# 数値と文字列を組み合わせて表示する\n",
        "print(f\"出力結果は{area}cm²です\")"
      ]
